
@model loja_Info.Models.modelCliente

@{
    ViewBag.Title = "cadCliente";
}

@using (Html.BeginForm("cadCliente", "Funcionario", FormMethod.Post))

{

<label>Cadastro de Cliente</label>
<br />


<div class="editor-label">
    @Html.LabelFor(m => m.nome_Cli)
</div>

<div class="editor-field">
    @Html.EditorFor(m => m.nome_Cli)
</div>

<div class="editor-label">
    @Html.LabelFor(m => m.email_Cli)
</div>

<div class="editor-field">
    @Html.EditorFor(m => m.email_Cli)
</div>

<div class="editor-label">
    @Html.LabelFor(m => m.endereco_Cli)
</div>

<div class="editor-field">
    @Html.EditorFor(m => m.endereco_Cli)
</div>

<div class="editor-label">
    @Html.LabelFor(m => m.cel_Cli)
</div>

<div class="editor-field">
    @Html.EditorFor(m => m.cel_Cli)
</div>


<br />
<input type="submit" value="Cadastrar" />
}
<br />
<br />

@ViewBag.confCadastro
